<html>
	<head>
		<% include ../partials/head %>
	</head>
	<body>
		<header>
			<% include ../partials/header %>
		</header>
		
		<main>
			<a href = "/places/add">
				<div id = "addButton" class = "button round clickable">
					+
				</div>
			</a>
			
			<% places.forEach(function(place){ %>
				<a href = "/places/<%= place.name %>">
					<div class = "placeWrapper iconWrapper clickable">
						<img src = "<%= place.img %>">
						<div class = "name"><%= place.name %></div>
					</div>
				</a>
			<% }); %>
		</main>
		
		<footer>
			<% include ../partials/footer %>
		</footer>
	</body>
</html>